@article{Demenech-etal2020,
  title = {Desigualdade econômica e risco de infecção e morte por COVID-19 no Brasil},
  author = {Demenech, Lauro Miranda and Dumith, Samuel de Carvalho and Vieira, Maria Eduarda Centena Duarte and Neiva-Silva, Lucas},
  date = {2020-10-05},
  journaltitle = {Revista Brasileira de Epidemiologia},
  shortjournal = {Rev. bras. epidemiol.},
  volume = {23},
  pages = {e200095},
  publisher = {Associação Brasileira de Pós -Graduação em Saúde Coletiva},
  issn = {1415-790X, 1415-790X},
  doi = {10.1590/1980-549720200095},
  url = {https://www.scielosp.org/article/rbepid/2020.v23/e200095/},
  urldate = {2021-01-12},
  abstract = {RESUMO: Objetivo: Avaliar, por meio de análise espaçotemporal, se a desigualdade econômica das Unidades Federativas (UF) do Brasil pode estar associada com o risco de infecção e morte por COVID-19. Métodos: Trata-se de um estudo ecológico, baseado em dados secundários das taxas de incidência e mortalidade para COVID-19. Os dados foram analisados em nível estadual, tendo como principal variável independente o coeficiente de Gini. Foram utilizados os registros de 12 dias, espaçados em uma semana cada, entre 21 de abril e 7 de julho de 2020. A variação semanal das taxas foi calculada pela regressão de Prais-Winsten, com o objetivo de medir a evolução da pandemia em cada UF. O teste de correlação de Spearman foi empregado para avaliar a correlação entre as taxas e suas evoluções semanais e as variáveis independentes. Por fim, realizou-se diagnóstico de dependência espacial dos dados e usou-se o modelo de defasagem da regressão espacial, quando aplicável. Resultados: As taxas de incidência e mortalidade por COVID-19 foram crescentes em todas as UF brasileiras, tendo sido mais acentuada entre aquelas com maior desigualdade econômica. A associação entre coeficiente de Gini e incidência e mortalidade por COVID-19 manteve-se mesmo quando levados em consideração aspectos demográficos e espaciais. Conclusão: A desigualdade econômica pode exercer papel importante no impacto da COVID-19 em território brasileiro, por meio de efeitos absolutos e contextuais. Políticas estruturais para a redução da desigualdade são fundamentais para o enfrentamento dessa e de futuras crises sanitárias no Brasil.},
  langid = {portuguese},
  keywords = {Coeficiente de Gini,COVID-19,Desigualdade em saúde,Determinantes sociais da saúde,Pandemia},
  annotation = {TLDR: Whether the income inequality of the Federative Units (FUs) in Brazil can be associated with the risk of infection and death by COVID-19 and whether structural policies to reduce inequality are essential to face this and future health crises in Brazil is assessed.},
  file = {G\:\\My Drive\\[[1]] Kami Uploads\\_Mov-Estud\\Demenech et al. - 2020 - Desigualdade econômica e risco de infecção e morte por COVID-19 no Brasil.pdf;C\:\\Users\\Mancano\\Zotero\\storage\\HAQRNAPN\\e200095.html}
}

