  title = {Vocational {{Training Institutions}} in {{Latin America}}: {{Imported}}, {{Indigenous}}, and/or {{Mestizo}}?},
  shorttitle = {Vocational {{Training Institutions}} in {{Latin America}}},
  booktitle = {Skills, {{Values}}, and {{Development}}},
  author = {Schrank, Andrew},
  editor = {Bogliaccini, Juan A and Madariaga, Aldo},
  date = {2025-10-24},
  edition = {1},
