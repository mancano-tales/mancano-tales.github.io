  doi = {10.1093/9780197803219.003.0009},
  url = {https://academic.oup.com/book/60850/chapter/529901568},
  urldate = {2025-09-07},
  abstract = {Abstract             Early childhood education and care (ECEC) as a mechanism to promote human capital formation has been at the center of the social investment agenda. Many Latin American countries have expanded childcare services and programs in the last two decades, a policy trend that might seem aligned with skill development goals. However, the characteristics of ECEC interventions vary notably across the region. This chapter explores the extent to which Latin American childcare programs have supported the skill-enhancing component in ECEC policies. The study considers two key dimensions to investigate variation in the implementation of this component: the delegation model to nongovernmental actors to provide the services, and the level of integration of ECEC programs into educational ministries. The author suggests that incumbent parties’ positions will inform policy design decisions over ECEC provision schemes on the ideological spectrum. Specifically, incumbent parties on the left will tend to favor direct provision or delegated schemes with higher levels of government oversight. In turn, political considerations regarding the strength of public unions help explain governments’ decisions regarding whether to embed ECEC services in education ministries. Governments dealing with teachers’ unions with significant political power will be more reluctant to integrate ECEC services into the education ministry. Focusing on two countries that expanded ECEC services in recent years, Chile and Mexico, the chapter aims to illuminate the factors underlying different strategies in implementing skills-enhancing reforms.},
  isbn = {978-0-19-780318-9 978-0-19-780321-9},
  langid = {english},
  file = {G:\My Drive\[[1]] Kami Uploads\Altamirano - 2025 - Early Childhood Education Policy Design, and Skills-Enhancing Goals in Latin America.pdf}
}

@article{Altonji,
