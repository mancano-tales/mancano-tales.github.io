@article{Dixit,
  title = {The {{Impact}} of {{Welfare}} on {{Inter-group Relations}}: {{Caste-based Social Insurance}} and {{Social Integration}} in {{India}}},
  author = {Dixit, Akshay Govind},
  abstract = {Religious and ethnic communities have long helped people cope with risks. Does welfare substitute for ethnicity-based social insurance? If so, does it improve inter-group relations? Ethnicity-based insurance functions on reciprocity and solidarity, requiring social investment in in-group ties. This limits the formation of out-group ties, exacerbating ethnic divisions. Welfare reduces dependence on the ethnic group, allowing individuals to form productive out-group ties and increasing inter-ethnic integration. I test this argument in caste networks in India. I leverage panel data on household loans to show that an income support program reduced within-caste borrowing by 38.5\%. Using survey data from 3,020 households, I show that welfare significantly enhanced inter-caste interaction, mainly in areas with lower caste-based land inequality. I draw on 56 qualitative interviews to document how ethnicity-based social insurance exacerbates social segregation, and why integration increases with lower group inequality. These findings illuminate the persistence and demise of ethnic divisions.},
  langid = {english},
  file = {G:\My Drive\[[1]] Kami Uploads\DixitThe Impact of Welfare on Inter-group Relations Caste-based Social Insurance and Social Integration in India.pdf}
}

