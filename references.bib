@article{Kang2011,
  title = {Descentralização e Financiamento Da Educação Brasileira: Uma Análise Comparativa, 1930-1964},
  shorttitle = {Descentralização e Financiamento Da Educação Brasileira},
  author = {Kang, Thomas H.},
  date = {2011-09},
  journaltitle = {Estudos Econômicos (São Paulo)},
