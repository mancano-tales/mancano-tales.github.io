  shorttitle = {Attitudes towards Student Support},
  author = {Garritzmann, Julian L},
  date = {2015-05},
  journaltitle = {Journal of European Social Policy},
  shortjournal = {Journal of European Social Policy},
  volume = {25},
  number = {2},
