@article{Charnysh-Finkel2017,
  title = {The {{Death Camp Eldorado}}: {{Political}} and {{Economic Effects}} of {{Mass Violence}}},
  shorttitle = {The {{Death Camp Eldorado}}},
  author = {Charnysh, Volha and Finkel, Evgeny},
  date = {2017-11},
  journaltitle = {American Political Science Review},
  shortjournal = {Am Polit Sci Rev},
  volume = {111},
