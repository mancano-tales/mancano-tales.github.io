@article{Grin2013a,
  title = {Regime de {{Bem-estar Social}} No {{Brasil}}: {{Três Períodos Históricos}}, {{Três Diferenças}} Em {{Relação}} Ao {{Modelo Europeu Social-democrata}}},
  shorttitle = {Regime de {{Bem-estar Social}} No {{Brasil}}},
  author = {Grin, Eduardo José},
