  title = {Rethinking {{Readings About}} the {{Past}} of {{School}} and {{Education Between Worlds}}},
  author = {Batista da Silva, Vivian and Gonçalves Vidal, Diana and Godinho Lima, Ana Laura and Bontempi Jr., Bruno},
  date = {2024-12-01},
